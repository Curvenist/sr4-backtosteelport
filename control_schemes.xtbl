			<Type>Element</Type>
			<Required>false</Required>
			</Element>
		</Element>
	<Element>
		<Name>Controls</Name>
		<Type>Grid</Type>
		<Element>
			<Name>Control</Name>
			<Type>Element</Type>
			<Element>
				<Name>Action</Name>
				<Type>Reference</Type>
				<Description>Action to bind the button to.</Description>
				<Reference>
					<File>control_mappings\control_actions.xtbl</File>
					<Type>Control_Action.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Button</Name>
				<Type>Reference</Type>
				<Description>Button bound to an action.</Description>
				<Default>UNBOUND</Default>
				<Reference>
					<File>control_mappings\buttons.xtbl</File>
					<Type>Button.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Alternate_Button</Name>
				<Type>Reference</Type>
				<Display_Name>Alternate Button</Display_Name>
				<Description>Alternate button bound to an action.</Description>
				<Default>UNBOUND</Default>
				<Reference>
					<File>control_mappings\buttons.xtbl</File>
					<Type>Button.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Stick_Direction</Name>
				<Type>Selection</Type>
				<Display_Name>Stick Direction</Display_Name>
				<Description>A stick direction you can bind to, acts as a button.</Description>
				<Default>STICK_DIR_UNBOUND</Default>
				<Choice>STICK_DIR_UNBOUND</Choice>
				<Choice>LEFT_STICK_DIR_UP</Choice>
				<Choice>LEFT_STICK_DIR_DOWN</Choice>
				<Choice>LEFT_STICK_DIR_RIGHT</Choice>
				<Choice>LEFT_STICK_DIR_LEFT</Choice>
				<Choice>RIGHT_STICK_DIR_UP</Choice>
				<Choice>RIGHT_STICK_DIR_DOWN</Choice>
				<Choice>RIGHT_STICK_DIR_RIGHT</Choice>
				<Choice>RIGHT_STICK_DIR_LEFT</Choice>
				</Element>
			<Element>
				<Name>Action_Name</Name>
				<Type>String</Type>
				<Display_Name>Action Name</Display_Name>
				<Description>Data name associated with the bound button. (Note: these will be converted to lowercase before stored.)</Description>
				<Required>false</Required>
				<MaxLength>23</MaxLength>
				</Element>
			<Element>
				<Name>X360</Name>
				<Type>Selection</Type>
				<Description>Whether this control is valid on Xbox 360.</Description>
				<Default>True</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>PS3</Name>
				<Type>Selection</Type>
				<Description>Whether this control is valid on PS3.</Description>
				<Default>True</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>PC</Name>
				<Type>Selection</Type>
				<Description>Whether this control is valid on PC.</Description>
				<Default>True</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>Debug_Only</Name>
				<Type>Selection</Type>
				<Display_Name>Debug Only</Display_Name>
				<Description>Whether this control is only valid in debug.</Description>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>Non_Release_Final</Name>
				<Type>Selection</Type>
				<Display_Name>Non-Release Final</Display_Name>
				<Description>Whether this control is invalid in a "Release Final" build.</Description>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			</Element>
		<Num_Display_Rows>500</Num_Display_Rows>
		</Element>
	<Element>
		<Name>Axes</Name>
		<Type>Grid</Type>
		<Element>
			<Name>Axis</Name>
			<Type>Element</Type>
			<Element>
				<Name>Action</Name>
				<Type>Reference</Type>
				<Description>Action to bind the axis to.</Description>
				<Reference>
					<File>control_mappings\control_axis_actions.xtbl</File>
					<Type>Control_Axis_Action.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Axis</Name>
				<Type>Selection</Type>
				<Description>Specifies a primary axis and stick.</Description>
				<Default>STICK_AXIS_UNBOUND</Default>
				<Choice>STICK_AXIS_UNBOUND</Choice>
				<Choice>RIGHT_STICK_AXIS_X</Choice>
				<Choice>RIGHT_STICK_AXIS_Y</Choice>
				<Choice>LEFT_STICK_AXIS_X</Choice>
				<Choice>LEFT_STICK_AXIS_Y</Choice>
				</Element>
			<Element>
				<Name>Inverted</Name>
				<Type>Selection</Type>
				<Description>Whether the primary axis is inverted.</Description>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>Alternate_Axis</Name>
				<Type>Selection</Type>
				<Display_Name>Alternate Axis</Display_Name>
				<Description>Specifies an alternate axis and stick.</Description>
				<Default>STICK_AXIS_UNBOUND</Default>
				<Choice>STICK_AXIS_UNBOUND</Choice>
				<Choice>RIGHT_STICK_AXIS_X</Choice>
				<Choice>RIGHT_STICK_AXIS_Y</Choice>
				<Choice>LEFT_STICK_AXIS_X</Choice>
				<Choice>LEFT_STICK_AXIS_Y</Choice>
				</Element>
			<Element>
				<Name>Alternate_Inverted</Name>
				<Type>Selection</Type>
				<Display_Name>Alternate Inverted</Display_Name>
				<Description>Whether the alternate axis is inverted.</Description>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			</Element>
		<Num_Display_Rows>500</Num_Display_Rows>
		</Element>
	<Single_Line_XML>False</Single_Line_XML>
	</TableDescription>

