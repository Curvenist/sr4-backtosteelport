					<Type>Sticks.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			</Element>
		<Element>
			<Name>Driving</Name>
			<Type>Element</Type>
			<Required>false</Required>
			<Element>
				<Name>Weapon_Selection_Stick</Name>
				<Type>Reference</Type>
				<Reference>
					<File>control_mappings\sticks.xtbl</File>
