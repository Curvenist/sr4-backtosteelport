		<MaxLength>23</MaxLength>
		</Element>
	<Element>
		<Name>Scheme_Type</Name>
		<Type>ComboElement</Type>
		<Display_Name>Scheme Type</Display_Name>
		<Element>
			<Name>General</Name>
			<Type>Element</Type>
			<Required>false</Required>
			</Element>
		<Element>
			<Name>On_Foot</Name>
			<Type>Element</Type>
			<Required>false</Required>
