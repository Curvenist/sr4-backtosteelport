	<Element><Name>Range_Max</Name><Type>Float</Type><Display_Name>Max. Range</Display_Name><Description>Maximum range of weapon (in meters, ignored for "thrown" weapons)</Description><Default>0.0</Default></Element>
	<Element><Name>AI_Ideal_Range_Min</Name><Type>Float</Type><Display_Name>AI Ideal Range Min</Display_Name><Description>The distance when a AI will start to back up.</Description><Default>5.0</Default><MinValue>0</MinValue></Element>
